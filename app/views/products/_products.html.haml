-@products.each do |product|
  .products_contain_item-box
    = link_to "", class: "link" do
      .products_contain_item-box-image
        -# %img{src: "data:image/jpeg;base64,", width: "210", height: "190"}
        -# = image_tag product.images.image
        = image_tag product.images[0].image.url
      .products_contain_item-box-body
        .products_contain_item-box-body-name
          -# Tシャツ
          = product.name
        .products_contain_item-box-body-under
          .products_contain_item-box-body-under-price
            = "¥#{product.price}"
          .products_contain_item-box-body-under-likes
            = icon "fas",'heart'
            1